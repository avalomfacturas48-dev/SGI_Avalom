  alq_monto            String                 @db.String(20)
  alq_fechapago        DateTime               @db.Date
  alq_contrato         String?                @db.String(50)
  alq_estado           String                 @db.String(1)
  alq_fechacreacion    DateTime?              @default(dbgenerated("current_date()")) @db.Date
